            <Button fx:id="btnYes" layoutX="75.0" layoutY="124.0" mnemonicParsing="false" onAction="#btnYesAction" text="Yes" />
            <Button fx:id="btnNo" mnemonicParsing="false" onAction="#btnNoAction" text="No" />
         </children>
      </HBox>
      <ImageView fitHeight="148.0" fitWidth="150.0" layoutX="75.0" layoutY="-41.0" scaleX="0.25" scaleY="0.25">
